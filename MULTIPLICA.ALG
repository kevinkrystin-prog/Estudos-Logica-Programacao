algoritmo "MULTIPLICA"
var
     N2, n1, S: INTEIRO
inicio
ESCREVA ("DIGITE O PRIMEIRO NUMERO: ")
         LEIA (N1)
         ESCREVA ("DIGITE O SEFUNDO NUMERO: ")
         LEIA (N2)
         S <- (N1 * N2)
         ESCREVA ("O RESULTADO DE MULTIPLICAÇAO: ", N1, "X", N2, " É IGUAL A: ", S)
         

fimalgoritmo
